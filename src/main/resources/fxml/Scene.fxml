<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<GridPane id="GridPane" prefHeight="500" prefWidth="800" xmlns:fx="http://javafx.com/fxml"
          fx:controller="ua.zig.Controller" alignment="center" hgap="10" vgap="10">
    <Button GridPane.rowIndex="0" GridPane.columnIndex="0" onAction="#handleButtonAction"
            fx:id="button">
        <graphic>
            <ImageView>
                <Image url="@/resources/start.jpg" requestedWidth="60" requestedHeight="40"/>
            </ImageView>
        </graphic>
    </Button>
    <Label  text="Finish Time" GridPane.rowIndex="0" GridPane.columnIndex="3" textFill="darkorange" />
    <Label  text="Finish ms" GridPane.rowIndex="0" GridPane.columnIndex="4" textFill="darkblue"
            prefWidth="69" prefHeight="16" alignment="CENTER"/>
    <Label  text="Top places" GridPane.rowIndex="0" GridPane.columnIndex="5" textFill="darkblue"
            prefWidth="69" prefHeight="16" alignment="CENTER"/>


    <Label text="Car #1" GridPane.rowIndex="1" GridPane.columnIndex="0" minHeight="16"
           minWidth="69"/>
    <Label fx:id="label1" GridPane.rowIndex="1" GridPane.columnIndex="1" minHeight="16" minWidth="69"/>
    <ProgressBar fx:id="progressBar1" GridPane.rowIndex="1" GridPane.columnIndex="2" prefWidth="300" />
    <Label fx:id="labelFinish1"  GridPane.rowIndex="1" GridPane.columnIndex="3" minHeight="16" minWidth="69"/>
    <Label fx:id="labelFinishTime1"  GridPane.rowIndex="1" GridPane.columnIndex="4" minHeight="16" minWidth="69"/>
    <Label fx:id="place1"  GridPane.rowIndex="1" GridPane.columnIndex="5" minHeight="16" minWidth="69"
        />

    <Label text="Car #2" GridPane.rowIndex="2" GridPane.columnIndex="0" minHeight="16"
           minWidth="69"/>
    <Label fx:id="label2" GridPane.rowIndex="2" GridPane.columnIndex="1" minHeight="16" minWidth="69"/>
    <ProgressBar fx:id="progressBar2" GridPane.rowIndex="2" GridPane.columnIndex="2" prefWidth="300" />
    <Label fx:id="labelFinish2"  GridPane.rowIndex="2" GridPane.columnIndex="3" minHeight="16" minWidth="69"/>
    <Label fx:id="labelFinishTime2"  GridPane.rowIndex="2" GridPane.columnIndex="4" minHeight="16" minWidth="69"/>
    <Label fx:id="place2" GridPane.rowIndex="2" GridPane.columnIndex="5" minHeight="16" minWidth="69"/>


    <Label text="Car #3" GridPane.rowIndex="3" GridPane.columnIndex="0" minHeight="16"
           minWidth="69"/>
    <Label fx:id="label3" GridPane.rowIndex="3" GridPane.columnIndex="1" minHeight="16" minWidth="69"/>
    <ProgressBar fx:id="progressBar3" GridPane.rowIndex="3" GridPane.columnIndex="2" prefWidth="300" />
    <Label fx:id="labelFinish3"  GridPane.rowIndex="3" GridPane.columnIndex="3" minHeight="16" minWidth="69"/>
    <Label fx:id="labelFinishTime3" GridPane.rowIndex="3" GridPane.columnIndex="4" minHeight="16" minWidth="69"/>
    <Label fx:id="place3" GridPane.rowIndex="3" GridPane.columnIndex="5" minHeight="16" minWidth="69"/>


    <Label text="Car #4" GridPane.rowIndex="4" GridPane.columnIndex="0" minHeight="16"
           minWidth="69"/>
    <Label fx:id="label4" GridPane.rowIndex="4" GridPane.columnIndex="1" minHeight="16" minWidth="69"/>
    <ProgressBar fx:id="progressBar4" GridPane.rowIndex="4" GridPane.columnIndex="2" prefWidth="300" />
    <Label fx:id="labelFinish4"  GridPane.rowIndex="4" GridPane.columnIndex="3" minHeight="16" minWidth="69"/>
    <Label fx:id="labelFinishTime4"  GridPane.rowIndex="4" GridPane.columnIndex="4" minHeight="16" minWidth="69"/>
    <Label fx:id="place4" GridPane.rowIndex="4" GridPane.columnIndex="5" minHeight="16" minWidth="69"/>


    <Label text="Car #5" GridPane.rowIndex="5" GridPane.columnIndex="0" minHeight="16"
           minWidth="69"/>
    <Label fx:id="label5" GridPane.rowIndex="5" GridPane.columnIndex="1" minHeight="16" minWidth="69"/>
    <ProgressBar fx:id="progressBar5" GridPane.rowIndex="5" GridPane.columnIndex="2" prefWidth="300" />
    <Label fx:id="labelFinish5"  GridPane.rowIndex="5" GridPane.columnIndex="3" minHeight="16" minWidth="69"/>
    <Label fx:id="labelFinishTime5"  GridPane.rowIndex="5" GridPane.columnIndex="4" minHeight="16" minWidth="69"/>
    <Label fx:id="place5" GridPane.rowIndex="5" GridPane.columnIndex="5" minHeight="16" minWidth="69"/>


    <Label text="Car #6" GridPane.rowIndex="6" GridPane.columnIndex="0" minHeight="16"
           minWidth="69"/>
    <Label fx:id="label6" GridPane.rowIndex="6" GridPane.columnIndex="1" minHeight="16" minWidth="69"/>
    <ProgressBar fx:id="progressBar6" GridPane.rowIndex="6" GridPane.columnIndex="2" prefWidth="300" />
    <Label fx:id="labelFinish6" GridPane.rowIndex="6" GridPane.columnIndex="3" minHeight="16" minWidth="69"/>
    <Label fx:id="labelFinishTime6" GridPane.rowIndex="6" GridPane.columnIndex="4" minHeight="16" minWidth="69"/>
    <Label fx:id="place6" GridPane.rowIndex="6" GridPane.columnIndex="5" minHeight="16" minWidth="69"/>

</GridPane>
